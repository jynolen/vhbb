// Placez vos paramètres dans ce fichier pour remplacer les paramètres par défaut et les paramètres utilisateur.
{
    "files.exclude": {
        "**/*.o": true,
        "bin/": true,
        "build/": true
    },
    "search.exclude": {
        "assets": true,
        "sce_sys": true,
        "tools": true,
        "CMakeLists.txt": true,
        "Makefile": true
    },
    "cppcheck.enable": true,
    "cppcheck.includePaths": [
        "/usr/local/vitasdk/arm-vita-eabi/include",
        ".",
        "src"
    ],
    "files.associations": {
        "array": "cpp",
        "xutility": "cpp",
        "system_error": "cpp",
        "functional": "cpp",
        "tuple": "cpp",
        "cctype": "cpp",
        "clocale": "cpp",
        "cstdarg": "cpp",
        "cstddef": "cpp",
        "cstdint": "cpp",
        "cstdio": "cpp",
        "cstdlib": "cpp",
        "ctime": "cpp",
        "cwchar": "cpp",
        "cwctype": "cpp",
        "exception": "cpp",
        "initializer_list": "cpp",
        "new": "cpp",
        "type_traits": "cpp",
        "typeinfo": "cpp",
        "*.ipp": "cpp"
    },
    "cmake.useCMakeServer": true
}
